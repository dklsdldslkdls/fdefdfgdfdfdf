{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "websocket:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:8080/client/auth"
        }
      ]
    },
    "process:default",
    "dialog:default"
  ]
}